    <bp:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UniProt:O15504 NUP42</bp:name>
    <bp:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NUP42</bp:name>
